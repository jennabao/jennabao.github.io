function math () {
	var answer = prompt("Enter 1+1:");
  if (answer == 2) {
  	confirm("You're not a complete failure *shocked*! Your answer was " + answer);
  }
  else {
  	confirm("You suck, tsk tsk tsk.")
  }
}
